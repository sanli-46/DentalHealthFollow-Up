<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    x:Class="DentalHealthFollow_Up.AppShell"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DentalHealthFollow_Up"
    Shell.FlyoutBehavior="Disabled"
    Title="DentalHealthFollow_Up">

    <ShellContent Title="Login" ContentTemplate="{DataTemplate local:LoginPage}" Route="LoginPage" />
    <ShellContent Title="Register" ContentTemplate="{DataTemplate local:RegisterPage}" Route="RegisterPage" />
    <ShellContent Title="Main" ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage" />
    <ShellContent Title="Profil" ContentTemplate="{DataTemplate local:ProfilePage}" Route="profile" />

</Shell>
