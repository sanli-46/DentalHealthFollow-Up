<?xml version="1.0" encoding="UTF-8" ?>
<Shell  
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:DentalHealthFollow_Up.MAUI"
    x:Class="DentalHealthFollow_Up.MAUI.AppShell"
    Shell.FlyoutBehavior="Disabled"
    Title="DentalHealthFollow_Up">


    <ShellContent Title="Login" Route="LoginPage" ContentTemplate="{DataTemplate views:LoginPage}" />
    <ShellContent Title="Register" Route="RegisterPage" ContentTemplate="{DataTemplate views:RegisterPage}" />
    <ShellContent Title="Main" Route="MainPage" ContentTemplate="{DataTemplate views:MainPage}" />
    <ShellContent Title="Profil" Route="ProfilePage" ContentTemplate="{DataTemplate views:ProfilePage}" />
    <ShellContent Title="Hedefler" Route="GoalsPage" ContentTemplate="{DataTemplate views:GoalsPage}" />
</Shell>

 